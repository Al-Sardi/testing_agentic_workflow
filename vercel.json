{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/upload",
            "dest": "server.js"
        },
        {
            "src": "/health",
            "dest": "server.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "index.html"
        }
    ]
}